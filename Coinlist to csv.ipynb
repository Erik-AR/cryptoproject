   "source": [
    "import requests\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "req = requests.get('https://www.cryptocompare.com/api/data/coinlist/').json()\n",
    "\n",
    "info = req['Data']\n",
    "\n",
    "coinList = pd.DataFrame(info)\n",
    "\n",
    "coinList = coinList.transpose()\n",
    "\n",
    "coinList.to_csv('coinList.csv')\n",
    "\n",
    "coinList.head()"
   ]
